{
  "manifest_version": 2,

  "name": "ProductivityExtension",
  "description": "Increase your productivity.",
  "version": "1.0",


"background": {"scripts":["background.js","background2.js","blocking.js"]},



  "permissions": [
          "tabs","topSites","webRequest",
          "webRequestBlocking",
          "*://*/*"
        ],



        

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },


  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
